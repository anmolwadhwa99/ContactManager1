<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="awad865.project.ContactManager1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />
	<uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/contacts_icon"
        android:label="@string/app_name"
        android:theme="@style/CustomTheme" >
        <activity
            android:name="awad865.project.ContactManager1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.Groups"
            android:label="@string/title_activity_groups" >
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.AddContact"
            android:label="Add Contact"
            android:parentActivityName="awad865.project.ContactManager1.MainActivity" >
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.AddGroup"
            android:label="Add Group"
            android:parentActivityName="awad865.project.ContactManager1.Groups" >
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.Favourites"
            android:label="@string/title_activity_favourites" >
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.OptionActivity"
            android:label="More Options"
            android:parentActivityName="awad865.project.ContactManager1.MainActivity" >
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.EditContact"
            android:label="@string/title_activity_edit_contact" 
            android:parentActivityName="awad865.project.ContactManager1.MainActivity">
        </activity>
        <activity
            android:name="awad865.project.ContactManager1.ViewContact"
            android:label="View Contact"
            android:parentActivityName="awad865.project.ContactManager1.MainActivity">
        </activity>
    </application>

</manifest>